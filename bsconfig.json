{
  "name": "chat-app",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "reason-react",
    "reason-apollo",
    "@glennsl/bs-json",
    "bs-webapi",
    "bs-promise-monad"
  ],
  "warnings": {
    "error": "+101"
  },
  "reason": {
    "react-jsx": 2
  },
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors", "-bs-g"],
  "namespace": true,
  "refmt": 3
}